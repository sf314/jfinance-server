FROM openjdk:8
RUN echo "Executing Dockerfile"
COPY ./build/libs/jfinance-rest-service-0.0.0.jar /home
WORKDIR /home
EXPOSE 8080 8080
CMD java -jar jfinance-rest-service-0.0.0.jar
# Sets default command of container to run the java exec after building
# Possibly balenalib/raspberrypi3-openjdk:8-jdk on pi